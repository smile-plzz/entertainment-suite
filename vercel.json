{
  "name": "entertainment-suite",
  "builds": [
    {
      "src": "src/api/check-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/fetch-news.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/omdb-proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/app.js",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/detail.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/utils/api.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check-video",
      "dest": "src/api/check-video.js"
    },
    {
      "src": "/api/fetch-news",
      "dest": "src/api/fetch-news.js"
    },
    {
      "src": "/api/omdb-proxy",
      "dest": "src/api/omdb-proxy.js"
    },
    {
      "src": "/src/detail.html",
      "dest": "src/detail.html"
    },
    {
      "src": "/src/detail.js",
      "dest": "src/detail.js"
    },
    {
      "src": "/src/utils/api.js",
      "dest": "src/utils/api.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}