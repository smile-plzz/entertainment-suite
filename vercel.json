{
  "name": "nowshowing-app",
  "builds": [
    {
      "src": "api/check-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/fetch-news.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/omdb-proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    },
    {
      "src": "carousels.js",
      "use": "@vercel/static"
    },
    {
      "src": "carousels.css",
      "use": "@vercel/static"
    },
    {
      "src": "news.html",
      "use": "@vercel/static"
    },
    {
      "src": "news.js",
      "use": "@vercel/static"
    },
    {
      "src": "news.css",
      "use": "@vercel/static"
    },
    {
      "src": "detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "detail.js",
      "use": "@vercel/static"
    },
    {
      "src": "detail.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/carousels.js",
      "dest": "/carousels.js"
    },
    {
      "src": "/carousels.css",
      "dest": "/carousels.css"
    },
    {
      "src": "/news.html",
      "dest": "/news.html"
    },
    {
      "src": "/news.js",
      "dest": "/news.js"
    },
    {
      "src": "/news.css",
      "dest": "/news.css"
    },
    {
      "src": "/detail.html",
      "dest": "/detail.html"
    },
    {
      "src": "/detail.js",
      "dest": "/detail.js"
    },
    {
      "src": "/detail.css",
      "dest": "/detail.css"
    },
    {
      "src": "/api/check-video",
      "dest": "/api/check-video.js"
    },
    {
      "src": "/api/fetch-news",
      "dest": "/api/fetch-news.js"
    },
    {
      "src": "/api/omdb-proxy",
      "dest": "/api/omdb-proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}