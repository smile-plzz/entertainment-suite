{
  "name": "entertainment-suite",
  "builds": [
    {
      "src": "src/api/check-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/fetch-news.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/api/omdb-proxy.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/app.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/carousels.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/carousels.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/news.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/news.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/news.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/detail.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/detail.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/now-showing/detail.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/script.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/movie-details.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/service-worker.js",
      "use": "@vercel/static"
    },
    {
      "src": "src/movie-recommendations/config.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check-video",
      "dest": "src/api/check-video.js"
    },
    {
      "src": "/api/fetch-news",
      "dest": "src/api/fetch-news.js"
    },
    {
      "src": "/api/omdb-proxy",
      "dest": "src/api/omdb-proxy.js"
    },
    {
      "src": "/src/now-showing/(.*)",
      "dest": "src/now-showing/$1"
    },
    {
      "src": "/src/movie-recommendations/(.*)",
      "dest": "src/movie-recommendations/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}